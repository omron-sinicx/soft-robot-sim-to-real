<mujoco model="robotiq_hande">
  <!-- hand-e URDF from https://github.com/cambel/robotiq -->
  <!-- then yuni loaded it in mujoco to convert to xml and used online tools to convert dae to stl -->
  <!-- then a bunch of manual copy and pasting from other gripper examples -->

  <asset>
    <mesh name="hand-e" file="meshes/robotiq_hand_e_gripper/hand-e.stl" />
    <mesh name="finger_1" file="meshes/robotiq_hand_e_gripper/finger_1.stl" />
    <mesh name="finger_2" file="meshes/robotiq_hand_e_gripper/finger_2.stl" />
    <!-- <mesh name="hand-e-collision" file="meshes/robotiq_hand_e_gripper/hand-e-collision.stl"/>
    <mesh name="finger_1-collision" file="meshes/robotiq_hand_e_gripper/finger_1-collision.stl"/>
    <mesh name="finger_2-collision" file="meshes/robotiq_hand_e_gripper/finger_2-collision.stl"/> -->
  </asset>

  <actuator>
    <position ctrllimited="true" ctrlrange="0 0.025" joint="finger_joint1" kp="1000" name="gripper_finger_joint1" forcelimited="true" forcerange="-20 20" />
    <position ctrllimited="true" ctrlrange="0 0.025" joint="finger_joint2" kp="1000" name="gripper_finger_joint2" forcelimited="true" forcerange="-20 20" />
  </actuator>

  <worldbody>
    <body name="gripper_base" pos="0 0 0">
      <geom type="cylinder" pos="0 0 0.0175" size="0.0315 0.0175" group="1" name="gripper_base_visual" rgba="1 1 1 1" />
      <geom type="cylinder" pos="0 0 0.045" size="0.0375 0.01" group="1" rgba="1 1 1 1" />
      <geom type="cylinder" pos="0 0 0.0175" size="0.0315 0.0175" group="0" name="gripper_base_collision"/>
      <geom type="cylinder" pos="0 0 0.045" size="0.0375 0.01" group="0" rgba="1 1 1 1" />
      <site name="ft_frame" pos="0 0 0" size="0.01 0.01 0.01" rgba="1 0 0 1" type="sphere" group="1" />
      <!-- This site was added for visualization. -->
      <body name="eef" pos="0 0 0" quat="0.707105 0 0 -0.707105">
        <site name="grip_site" pos="0 0 0" size="0.01 0.01 0.01" rgba="1 0 0 0.5" type="sphere" group="1" />
        <site name="ee_x" pos="0.1 0 0" size="0.005 .1" quat="0.707105  0 0.707108 0 " rgba="1 0 0 0" type="cylinder" group="1" />
        <site name="ee_y" pos="0 0.1 0" size="0.005 .1" quat="0.707105 0.707108 0 0" rgba="0 1 0 0" type="cylinder" group="1" />
        <site name="ee_z" pos="0 0 0.1" size="0.005 .1" quat="1 0 0 0" rgba="0 0 1 0" type="cylinder" group="1" />
        <!-- This site was added for visualization. -->
        <site name="grip_site_cylinder" pos="0 0 0" size="0.005 10" rgba="0 1 0 0.3" type="cylinder" group="1" />
      </body>
      <body name="right_gripper" pos="0 0 0.055" quat="0.707107 0 0 -0.707107">
        <!-- inertia manually copied over from URDF, not autogenerated via mujoco -->
        <inertial pos="0 0 0.04607" mass="0.86387" diaginertia="1017560E-9 1028041E-9 489810E-9" />
        <geom type="mesh" contype="0" conaffinity="0" group="1" mesh="hand-e" name="hand_visual" rgba="0.1 0.1 0.1 1" />
        <!-- <geom type="mesh" mesh="hand-e-collision"  group="0" name="hand_collision"/> -->
        <geom type="cylinder" pos="0 0 0.049" size="0.0375 0.049" group="0" name="hand_collision" />
        <body name="leftfinger" pos="0 0 0.099">
          <inertial pos="0.02262 -0.00759 0.00738" quat="0.871952 0.0369732 -0.211073 0.440205" mass="0.03804" diaginertia="1.22763e-05 1.22763e-05 1.22763e-05" />
          <joint name="finger_joint1" pos="0 0 0" axis="-1 0 0" type="slide" limited="true" range="0 0.025" damping="100" armature="1.0" frictionloss="1.0" />
          <geom type="mesh" contype="0" conaffinity="0" group="1" mesh="finger_1" name="finger1_visual" rgba="1 1 1 1" />
          <!-- <geom type="mesh" group="0" conaffinity="1" contype="0" solref="0.02 1" friction="1 0.005 0.0001" condim="4" mesh="finger_1-collision" name="finger1_collision"/> -->
          <geom type="box" pos="0.030677 0.0 0.02325" size="0.0054 0.0105 0.02325" group="0" conaffinity="1" contype="0" solref="0.02 1" friction="1 0.005 0.0001" condim="4" name="finger1_collision" />
        </body>
        <body name="rightfinger" pos="0 0 0.099">
          <inertial pos="-0.02262 0.00759 0.00738" quat="0.871952 -0.0369732 0.211073 0.440205" mass="0.03804" diaginertia="1.22763e-05 1.22763e-05 1.22763e-05" />
          <joint name="finger_joint2" pos="0 0 0" axis="1 0 0" type="slide" limited="true" range="0 0.025" damping="100" armature="1.0" frictionloss="1.0" />
          <geom type="mesh" contype="0" conaffinity="0" group="1" mesh="finger_2" name="finger2_visual" rgba="1 1 1 1" />
          <!-- <geom type="mesh" group="0" conaffinity="1" contype="0" solref="0.02 1" friction="1 0.005 0.0001" condim="4" mesh="finger_2-collision" name="finger2_collision"/> -->
          <geom type="box" pos="-0.030677 0.0 0.02325" size="0.0054 0.0105 0.02325" group="0" conaffinity="1" contype="0" solref="0.02 1" friction="1 0.005 0.0001" condim="4" name="finger2_collision" />
        </body>
      </body>
    </body>
  </worldbody>

  <sensor>
    <force name="force_ee" site="ft_frame" />
    <torque name="torque_ee" site="ft_frame" />
  </sensor>

</mujoco>